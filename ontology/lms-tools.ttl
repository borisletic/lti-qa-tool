# DATA PROPERTIES - Tool Properties
# ============================================

:ltiVersion rdf:type owl:DatatypeProperty ;
    rdfs:domain :LTIProvider ;
    rdfs:range xsd:string ;
    rdfs:label "LTI version"@en .

:deploymentId rdf:type owl:DatatypeProperty ;
    rdfs:domain :LTIProvider ;
    rdfs:range xsd:string ;
    rdfs:label "deployment ID"@en .

:clientId rdf:type owl:DatatypeProperty ;
    rdfs:domain :LTIProvider ;
    rdfs:range xsd:string ;
    rdfs:label "client ID"@en .

:versionNumber rdf:type owl:DatatypeProperty ;
    rdfs:domain :LTIStandard ;
    rdfs:range xsd:string ;
    rdfs:label "version number"@en .

:usesAuthentication rdf:type owl:DatatypeProperty ;
    rdfs:domain :LTIStandard ;
    rdfs:range xsd:string ;
    rdfs:label "uses authentication"@en .

# ============================================
# DATA PROPERTIES - User Properties
# ============================================

:userId rdf:type owl:DatatypeProperty ;
    rdfs:domain :User ;
    rdfs:range xsd:string ;
    rdfs:label "user ID"@en .

:userName rdf:type owl:DatatypeProperty ;
    rdfs:domain :User ;
    rdfs:range xsd:string ;
    rdfs:label "user name"@en .

:email rdf:type owl:DatatypeProperty ;
    rdfs:domain :User ;
    rdfs:range xsd:string ;
    rdfs:label "email"@en .

# ============================================
# DATA PROPERTIES - Course Properties
# ============================================

:courseId rdf:type owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:string ;
    rdfs:label "course ID"@en .

:courseTitle rdf:type owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:string ;
    rdfs:label "course title"@en .

:courseDescription rdf:type owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:string ;
    rdfs:label "course description"@en .

# ============================================
# DATA PROPERTIES - Q&A Properties
# ============================================

:questionText rdf:type owl:DatatypeProperty ;
    rdfs:domain :Question ;
    rdfs:range rdf:langString ;
    rdfs:label "question text"@en ;
    rdfs:label "tekst pitanja"@sr .

:answerText rdf:type owl:DatatypeProperty ;
    rdfs:domain :Answer ;
    rdfs:range rdf:langString ;
    rdfs:label "answer text"@en ;
    rdfs:label "tekst odgovora"@sr .

:confidenceScore rdf:type owl:DatatypeProperty ;
    rdfs:domain :Answer ;
    rdfs:range xsd:float ;
    rdfs:label "confidence score"@en ;
    rdfs:label "skorovi poverenja"@sr ;
    rdfs:comment "Confidence level of the answer (0.0 to 1.0)"@en .

:timestamp rdf:type owl:DatatypeProperty ;
    rdfs:range xsd:dateTime ;
    rdfs:label "timestamp"@en .

:rating rdf:type owl:DatatypeProperty ;
    rdfs:domain :Feedback ;
    rdfs:range xsd:integer ;
    rdfs:label "rating"@en ;
    rdfs:comment "Rating from 1 to 5"@en .

:comment rdf:type owl:DatatypeProperty ;
    rdfs:domain :Feedback ;
    rdfs:range xsd:string ;
    rdfs:label "comment"@en .

:generatedAt rdf:type owl:DatatypeProperty ;
    rdfs:domain :Answer ;
    rdfs:range xsd:dateTime ;
    rdfs:label "generated at"@en .

# ============================================
# INDIVIDUALS - Example LTI Standards
# ============================================

:Canvas rdf:type :LTIConsumer ;
    rdfs:label "Canvas LMS" ;
    :implementsStandard :LTI_1_3 ;
    rdfs:comment "Instructure Canvas Learning Management System"@en .

:Moodle rdf:type :LTIConsumer ;
    rdfs:label "Moodle" ;
    :implementsStandard :LTI_1_3 ;
    rdfs:comment "Open-source Learning Management System"@en .

# ============================================
# AXIOMS & RESTRICTIONS
# ============================================

# A Question must be asked by exactly one User
:Question rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :askedBy ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
] .

# An Answer must answer exactly one Question
:Answer rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :answersQuestion ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
] .

# A ToolLaunch must involve exactly one tool, one user, and one course
:ToolLaunch rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :launchedTool ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
] .

:ToolLaunch rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :byUser ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
] .

# Confidence score must be between 0 and 1
:Answer rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :confidenceScore ;
    owl:allValuesFrom [
        rdf:type rdfs:Datatype ;
        owl:onDatatype xsd:float ;
        owl:withRestrictions (
            [ xsd:minInclusive "0.0"^^xsd:float ]
            [ xsd:maxInclusive "1.0"^^xsd:float ]
        )
    ]
] .

# ============================================
# END OF ONTOLOGY
